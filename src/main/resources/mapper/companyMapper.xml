<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Company">
    <insert id="save" parameterType="company">
        insert into company_table (companyId,companyPassword,companyName,companyEmail,companyMobile,companyNumber,companyLocation)
                values (#{companyId},#{companyPassword},#{companyName},#{companyEmail},#{companyMobile},#{companyNumber},#{companyLocation})
    </insert>
    <select id="login" resultType="company">
        select * from company_table where companyId=#{companyId} and companyPassword=#{companyPassword}
    </select>
</mapper>
